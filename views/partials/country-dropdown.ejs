<div class="ui fluid search selection dropdown">
    <input id="country-val" type="hidden" name="country" value="<%=countryData.slug%>">
    <i class="dropdown icon"></i>
    <div class="default text"><i class="<%=countryData.slug%> flag"></i><%=countryData.name%></div>
    <div class="menu">

    <% countryList.forEach( (element) => { %>
        <div class="item" data-value="<%=element.slug%>"><i class="<%=element.iso%> flag"></i><%=element.name%></div>
    <% }) %>
 
    </div>
</div>